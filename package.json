{
    "name": "cppo",
    "version": "1.4.1",
    "dependencies": {
        "ocamlfind": "andreypopp/ocamlfind#esy",
        "opam-installer-bin": "andreypopp/opam-installer-bin"
    },
    "esy": {
        "build": [
            "make opt",
            "make ocamlbuild",
            "make BINDIR=$cur__bin install-bin",
            "make install-lib",
            "(opam-installer --prefix=$cur__install || true)"
        ],
        "exportedEnv": {
            "cppo_installed": {
                "scope": "global",
                "val": "true"
            },
            "cppo_enable": {
                "scope": "global",
                "val": "enable"
            },
            "cppo_version": {
                "scope": "global",
                "val": "1.4.1"
            }
        }
    },
    "peerDependencies": {
        "ocaml": "andreypopp/ocaml#esy"
    }
}
